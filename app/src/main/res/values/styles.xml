<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonMainStyle">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_Size_22sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="TextSettingsStyle">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/paddingTop_21dp</item>
        <item name="android:paddingBottom">@dimen/paddingBotton_21dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/settings_text_16sp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingStart">@dimen/paddingTop_21dp</item>
        <item name="android:paddingEnd">@dimen/paddingTop_21dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="thumbTint">@color/thumbTint</item>
        <item name="trackTint">@color/trackTint</item>
    </style>

    <style name="ButtonSettingsStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_Size_22sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="ButtonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/paddingTop_21dp</item>
        <item name="android:paddingBottom">@dimen/paddingBotton_21dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">start</item>
        <item name="android:textSize">@dimen/settings_text_16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_marginStart">@dimen/layout_marginLeft_16dp</item>
        <item name="android:layout_marginEnd">@dimen/layout_marginRight_16dp</item>
        <item name="android:layout_marginTop">@dimen/layout_marginTop_8dp</item>
        <item name="android:hint">@string/search</item>
        <item name="android:paddingStart">@dimen/paddingStart_16dp</item>
        <item name="android:paddingEnd">@dimen/paddingEnd_16dp</item>
        <item name="android:textColorHint">@color/textColorHint</item>
        <item name="android:textSize">@dimen/settings_text_16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:drawablePadding">@dimen/drawablePadding_9dp</item>
        <item name="android:inputType">text</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="SwitchCompatStyle">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_marginEnd">@dimen/layout_marginEnd_18dp</item>
        <item name="android:thumbTint">#AEAFB4</item>
        <item name="android:trackTint">#E6E8EB</item>
    </style>

    <style name="ImageViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginEnd">30dp</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:visibility">gone</item>
        <item name="android:tint">@color/textColorHint</item>
    </style>

    <style name="pattern_track">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="pattern_track_tint">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/textColorHint</item>
    </style>

    <style name="buttonClear">
        <item name="cornerRadius">54dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="historyButton">
        <item name="cornerRadius">54dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="historyViewSearch">
        <item name="android:textSize">19sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="buttonBackPlayer">
        <item name="android:tint">@color/black</item>
    </style>

    <style name="textColor">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="textColorPlaylistInfo">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="tabLayout">
        <item name="android:textSize">14sp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="tabSelectedTextColor">@color/black</item>
        <item name="tabIndicatorFullWidth">false</item>
        <item name="tabIndicatorColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>

    <style name="TextViewStyle">
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/text_Size_22sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="colorLine">
        <item name="android:background">@color/lightGrey</item>
    </style>

    <style name="bottom_navigation_menu">
        <item name="itemRippleColor">@android:color/transparent</item>
        <item name="itemActiveIndicatorStyle">@color/white</item>
        <item name="android:background">@color/white</item>
        <item name="android:textSize">@dimen/ts_16sp</item>
    </style>

    <style name="new_playlist_editTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_marginStart">@dimen/layout_marginLeft_16dp</item>
        <item name="android:layout_marginEnd">@dimen/layout_marginRight_16dp</item>
        <item name="android:layout_marginTop">@dimen/layout_marginTop_8dp</item>
        <item name="android:hint">@string/search</item>
        <item name="android:paddingStart">@dimen/paddingStart_16dp</item>
        <item name="android:paddingEnd">@dimen/paddingEnd_16dp</item>
        <item name="android:textColorHint">@color/textColorHint</item>
        <item name="android:textSize">@dimen/settings_text_16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:drawablePadding">@dimen/drawablePadding_9dp</item>
        <item name="android:inputType">text</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="textInputLayout">
        <item name="android:layout_width">328dp</item>
        <item name="android:layout_height">56dp</item>
        <item name="android:layout_marginStart">@dimen/layout_marginLeft_16dp</item>
        <item name="android:layout_marginEnd">@dimen/layout_marginRight_16dp</item>
        <item name="android:textColorHint">@color/black</item>
    </style>

    <style name="textInputEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:inputType">text</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="itemPlaylist">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="textColorPlaylistInfoMenu">
        <item name="android:textColor">@color/black</item>
    </style>
</resources>